+    """
+    A Standard ML lexer.
+    """
+    name = 'Standard ML'
+    aliases = ['sml']
+    filenames = ['*.sml','*.sig','*.fun','*.ML']
+    mimetypes = ['text/x-standardml', 'application/x-standardml']
