Redirect permanent /HTML/node18.html http://www.mlton.org/user-guide/Cross_compiling_application.html
Redirect permanent /HTML/node33.html http://www.mlton.org/user-guide/MLton_Process.html
Redirect permanent /HTML/node59.html http://www.mlton.org/user-guide/Deviations_SML_NJ_from.html
#RedirectMatch permanent /HTML* http://www.mlton.org/user-guide/index.html

